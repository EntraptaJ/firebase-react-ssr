{
  "name": "react-firebase",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": "10"
  },
  "main": "functions/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "ts-node --project bin/tsconfig.json bin/build",
    "watch": "ts-node --project bin/tsconfig.json bin/watch"
  },
  "alias": {
    "ui": "./ui",
    "functions": "./functions"
  },
  "browserslist": [
    "last 1 Chrome version"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@types/express": "^4.17.0",
    "@types/fs-extra": "^8.0.0",
    "@types/node": "^12.7.1",
    "@types/parcel-bundler": "^1.12.0",
    "@types/react": "^16.9.1",
    "@types/react-dom": "^16.8.5",
    "@types/react-loadable": "^5.5.1",
    "@types/react-router-dom": "^4.3.4",
    "@types/sharp": "^0.22.2",
    "@types/workbox-sw": "^4.2.0",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "eslint-plugin-standard": "^4.0.0",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-assets-list": "^1.7.1",
    "parcel-plugin-bundle-manifest": "^0.2.0",
    "parcel-plugin-compress": "^1.0.10",
    "prettier": "^1.18.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "extends": [
      "standard",
      "plugin:react/recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier",
      "prettier/standard",
      "prettier/@typescript-eslint",
      "prettier/react"
    ],
    "plugins": [
      "react-hooks"
    ],
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "rules": {
      "@typescript-eslint/no-explicit-any": "off",
      "@typescript-eslint/explicit-function-return-type": "off",
      "@typescript-eslint/no-var-requires": "warn",
      "@typescript-eslint/no-unused-vars": [
        "error",
        {
          "vars": "all",
          "args": "none",
          "ignoreRestSiblings": true
        }
      ]
    }
  },
  "dependencies": {
    "@apollo/react-ssr": "^3.0.0",
    "@firebase/app": "^0.4.14",
    "@firebase/app-types": "^0.4.3",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link-http": "^1.5.15",
    "apollo-server-cloud-functions": "^2.8.1",
    "firebase-admin": "^8.3.0",
    "firebase-firestorm": "^1.0.2",
    "firebase-functions": "^3.2.0",
    "fs-extra": "^8.1.0",
    "graphql": "^14.4.2",
    "isomorphic-unfetch": "^3.0.0",
    "multistream": "^4.0.0",
    "react": "^16.9.0",
    "react-cookie": "^4.0.1",
    "react-dom": "^16.9.0",
    "react-loadable": "^5.5.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.23.0",
    "type-graphql": "^0.17.4",
    "universal-cookie-express": "^4.0.1"
  }
}
